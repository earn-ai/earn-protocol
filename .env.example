# Earn Protocol Environment Variables

# Required
EARN_WALLET_KEY=         # Base58 encoded private key for the Earn wallet

# Network (devnet or mainnet RPC)
RPC_URL=https://api.devnet.solana.com

# Optional - Supabase (for persistent storage)
SUPABASE_URL=            # Your Supabase project URL
SUPABASE_KEY=            # Your Supabase anon key

# Optional - IPFS (for image uploads)
NFT_STORAGE_KEY=         # NFT.Storage API key for IPFS uploads

# Optional - Helius (for enhanced token data)
HELIUS_API_KEY=          # Helius API key for metadata

# Advanced
RPC_COMMITMENT=confirmed
RPC_TIMEOUT_MS=30000
PORT=3000
