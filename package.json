{
  "name": "@earn-protocol/sdk",
  "version": "0.3.0",
  "description": "Tokenomics-as-a-service for memecoins - SDK & API server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sdk": {
      "import": "./dist/sdk.js",
      "require": "./dist/sdk.js",
      "types": "./dist/sdk.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "start": "node dist/index.js",
    "deploy:devnet": "./scripts/deploy-devnet.sh",
    "register:token": "ts-node scripts/register-test-token.ts",
    "test:devnet": "ts-node scripts/register-test-token.ts"
  },
  "keywords": ["solana", "tokenomics", "memecoin", "staking", "defi", "sdk"],
  "author": "Earn Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/earn-ai/earn-protocol"
  },
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "express": "^4.18.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.2"
  }
}
